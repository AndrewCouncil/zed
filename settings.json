{
  // ai settings
  "assistant": {
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-5-sonnet-latest"
    },
    "version": "2"
  },
  "features": {
    "edit_prediction_provider": "zed"
  },

  // extensions
  "auto_install_extensions": {
    "basher": true,
    "catppuccin": true,
    "catppuccin-icons": true,
    "csv": true,
    "docker-compose": true,
    "dockerfile": true,
    "env": true,
    "git-firefly": true,
    "gosum": true,
    "html": true,
    "latex": true,
    "log": true,
    "lua": true,
    "make": true,
    "markdown-oxide": true,
    "neocmake": true,
    "nginx": true,
    "nix": true,
    "nu": true,
    "proto": true,
    "sql": true,
    "terraform": true,
    "toml": true,
    "xml": true,
    "zig": true
  },

  // editor settings
  "autosave": {
    "after_delay": {
      "milliseconds": 500
    }
  },
  "base_keymap": "VSCode",
  "buffer_font_family": "JetBrainsMono Nerd Font",
  "buffer_font_size": 16,
  "cursor_blink": false,
  "file_types": {
    "CMake": ["CMakeLists.txt", "cmake"]
  },
  "icon_theme": {
    "dark": "Catppuccin Mocha",
    "light": "Catppuccin Latte",
    "mode": "dark"
  },
  "languages": {
    "Markdown": {
      "soft_wrap": "editor_width"
    }
  },
  "load_direnv": "shell_hook",
  "relative_line_numbers": true,
  "theme": {
    "dark": "Catppuccin Mocha",
    "light": "Catppuccin Mocha",
    "mode": "dark"
  },
  "ui_font_size": 16,
  "vim_mode": true
}
