{
  "assistant": {
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-5-sonnet-latest"
    },
    "version": "2"
  },
  "auto_install_extensions": {
    "catppuccin": true,
    "catppuccin-icons": true,
    "nix": true,
    "proto": true,
    "neocmake": true,
    "dockerfile": true,
    "nu": true,
    "html": true
  },
  "autosave": {
    "after_delay": {
      "milliseconds": 500
    }
  },
  "base_keymap": "VSCode",
  "buffer_font_family": "JetBrainsMono Nerd Font",
  "buffer_font_size": 16,
  "cursor_blink": false,
  "features": {
    "edit_prediction_provider": "zed"
  },
  "file_types": {
    "CMake": [
      "CMakeLists.txt",
      "cmake"
    ]
  },
  "icon_theme": {
    "dark": "Catppuccin Mocha",
    "light": "Catppuccin Latte",
    "mode": "dark"
  },
  "languages": {
    "Markdown": {
      "soft_wrap": "editor_width"
    }
  },
  "load_direnv": "shell_hook",
  "relative_line_numbers": true,
  "theme": {
    "dark": "Catppuccin Mocha",
    "light": "Catppuccin Mocha",
    "mode": "dark"
  },
  "ui_font_size": 16,
  "vim_mode": true
}
